#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules/@nuxt/ui/cli/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules/@nuxt/ui/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules/@nuxt/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules/@nuxt/ui/cli/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules/@nuxt/ui/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules/@nuxt/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/@nuxt+ui@3.2.0_@babel+parse_ad3ebd9957ecd300163fa4ccb617f7f5/node_modules:/mnt/c/Users/goktu/OneDrive/Belgeler/Masaüstü/vue1/user-todo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli/index.mjs" "$@"
else
  exec node  "$basedir/../../cli/index.mjs" "$@"
fi
